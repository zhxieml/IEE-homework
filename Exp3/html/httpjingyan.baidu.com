!function(){    function monitorHTTPWarning(ignore) {
        var cache = {};

        var selector = ['link', 'script', 'img', 'embed', 'iframe'];
        var knownHost = ['jingyan.baidu.com', 'hm.baidu.com', 'exp.bdstatic.com', 'himg.bdimg.com', 'img.baidu.com', 'kstj.baidu.com', 'hiphotos.baidu.com'];

        function monitor() {
            selector.reduce(function (partial, v) {
                var arr = document.getElementsByTagName(v);
                for (var i = 0, len = arr.length; i < len; i++) {
                    partial.push(arr[i]);
                }
                return partial;
            }, [])
            .forEach(function (v) {
                var url = v.href || v.src || '';
                // 只统计http开头
                if (url.indexOf('http:') !== 0) {
                    return 1;
                }

                // 避免重复发请求
                if (cache[url]) {
                    cache[url] += 1;
                    return 2;
                }
                
                // 忽略已知域名
                if (ignore && knownHost.some(function (v) {return ~url.indexOf(v)})) {
                    return 3;
                }
                cache[url] = 1;
                var img = new Image();
                img.src = '//kstj.baidu.com/v.gif?'
                    + 'pid=180&type=2000&module=common&page=https&action=warning&t=' + Date.now()
                    + '&curl=' + encodeURIComponent(location.href)
                    + '&rurl=' + encodeURIComponent(url)
            });
        }

        // 1 8 27 64 125
        for(var i = 1; i < 6; i++) {
            (function (i){
                setTimeout(function(){
                    monitor();
                }, i*i*i*1000);
            })(i);
        }
    }

    if (Math.floor(Math.random() * 100) < 100) {
        monitorHTTPWarning(0);
    }
}();</script></html>
